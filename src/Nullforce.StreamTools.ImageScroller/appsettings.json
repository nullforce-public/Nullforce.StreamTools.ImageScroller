{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "ScrollSettings": {
    "FontColor": "black",
    "ImageCount": 3,
    "ImageLocation": "C:\\images",
    "ImageMaxHeight": 200,
    "ImageMaxWidth": 200,
    "IntervalInSeconds": 5,
    "ShowFilename": true,
    "ShuffleImages": true
  }
}
